<%- include("partials/header") -%>

<h1>Compose</h1>
<form class="" action="compose" method="post">
  <div class="form-group">
    <label>Title</label>
      <input type="text" class="form-control" name="postTitle" placeholder="Put your titile here">
  </div>
  <div class="form-group">
    <label>Post</label>
      <textarea class="form-control" placeholder="Put your daily journal here" name="postBody" rows="10"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="publish">Publish</button>
</form>

<%- include("partials/footer") -%>
